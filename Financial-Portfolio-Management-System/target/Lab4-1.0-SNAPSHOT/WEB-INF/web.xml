<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>torontomu.ca.lab4.Business.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>MenuServlet</servlet-name>
        <servlet-class>torontomu.ca.lab4.Business.MenuServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MenuServlet</servlet-name>
        <url-pattern>/menu</url-pattern>
    </servlet-mapping>
<servlet>
        <servlet-name>TradingServlet</servlet-name>
        <servlet-class>torontomu.ca.lab4.Business.TradingServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TradingServlet</servlet-name>
        <url-pattern>/trading</url-pattern>
    </servlet-mapping>
</web-app>